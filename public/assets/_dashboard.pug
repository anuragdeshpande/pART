extends partials/_sidebar
block mainPanel
    nav.navbar.navbar-expand-lg.navbar-transparent.navbar-absolute.bg-primary
        .container-fluid
            .navbar-wrapper
                .navbar-toggle
                    button.navbar-toggler(type='button')
                        span.navbar-toggler-bar.bar1
                        span.navbar-toggler-bar.bar2
                        span.navbar-toggler-bar.bar3
                a.navbar-brand(href='#pablo') #{pageName}
